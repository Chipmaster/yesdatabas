<%= stylesheet_link_tag :all %>

<h1>File Upload</h1>
<%= form_tag ({:action => 'uploadFile'}, 
                        :multipart => true) %>
<p><label for="upload_file">Select CSS File</label> : 
<%= file_field 'upload', 'datafile' %></p>
<%= submit_tag "Upload" %>
<%= link_to 'Back', users_path %>